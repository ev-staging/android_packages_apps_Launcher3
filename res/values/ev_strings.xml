<resources xmlns:xliff="urn:oasis:names:tc:xliff:document:1.2">

    <!-- Settings title to show Google Now at -1 screen on launcher. [CHAR LIMIT=50] -->
    <string name="enable_minus_one_title">Show Google App</string>
    <!-- Settings message explaining when the -1 screen is available on an LTR device. [CHAR LIMIT=100] -->
    <string name="enable_minus_one_desc">When you swipe left from home screen</string>

    <!-- Label of tab to indicate google apps -->
    <string name="google_title" translatable="false">Google</string>
    <string name="play_folder_title">Play</string>

    <!-- Categories -->
    <string name="settings_category_home">Home screen settings</string>
    <string name="settings_category_icons">Icons settings</string>

    <!-- Search bar visibility setting -->
    <string name="enable_qsb_widget_title">Search bar on home screen</string>
    <string name="enable_qsb_widget_desc">Toggle the search bar visibility on the home screen</string>

</resources>
